{
  "name": "eslint-ja-proxy",
  "version": "0.1.0",
  "description": "ESLintのエラーメッセージを日本語化するプロキシ",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./register": "./dist/register.cjs"
  },
  "bin": {
    "eslint-ja": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "dict",
    "schemas"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "test": "vitest",
    "generate-keys": "tsx scripts/generate-keys.ts"
  },
  "keywords": [
    "eslint",
    "japanese",
    "i18n",
    "translate"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "semver": "^7.5.0"
  }
}
